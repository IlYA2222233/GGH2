-- ĞœĞ¾Ğ´ÑƒĞ»ÑŒ ReplicatedStorage Exploit
local StorageKick = {
    ForceDesync = function(targetPlayer)
        -- ĞŸĞ¾Ğ¸ÑĞº ÑƒÑĞ·Ğ²Ğ¸Ğ¼Ñ‹Ñ… RemoteEvents/Functions
        local criticalRemotes = {}
        
        local function findCritical(obj)
            if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
                local blacklistedNames = {
                    "Kick", "Ban", "Admin", "Mod", "Delete", 
                    "Remove", "Destroy", "Teleport", "God"
                }
                
                for _, name in pairs(blacklistedNames) do
                    if string.find(obj.Name:lower(), name:lower()) then
                        table.insert(criticalRemotes, obj)
                        break
                    end
                end
            end
        end
        
        game:GetService("ReplicatedStorage"):GetDescendants(findCritical)
        
        -- ĞÑ‚Ğ°ĞºĞ° Ğ½Ğ° Ğ½Ğ°Ğ¹Ğ´ĞµĞ½Ğ½Ñ‹Ğµ Ñ‚Ğ¾Ñ‡ĞºĞ¸
        for _, remote in pairs(criticalRemotes) do
            pcall(function()
                if remote:IsA("RemoteEvent") then
                    -- ĞÑ‚Ğ¿Ñ€Ğ°Ğ²ĞºĞ° Ğ´Ğ°Ğ½Ğ½Ñ‹Ñ…, Ğ²Ñ‹Ğ·Ñ‹Ğ²Ğ°ÑÑ‰Ğ¸Ñ… Ğ¾ÑˆĞ¸Ğ±ĞºÑƒ ÑĞµÑ€Ğ²ĞµÑ€Ğ°
                    for i = 1, 10 do
                        remote:FireServer({
                            Player = targetPlayer.Name,
                            UserId = targetPlayer.UserId,
                            Reason = "CHEATING",
                            Timestamp = os.time(),
                            Data = string.rep("FLOOD", 1000)
                        })
                    end
                elseif remote:IsA("RemoteFunction") then
                    -- Ğ’Ñ‹Ğ·Ğ¾Ğ² Ñ„ÑƒĞ½ĞºÑ†Ğ¸Ğ¹ Ñ Ğ½ĞµĞºĞ¾Ñ€Ñ€ĞµĞºÑ‚Ğ½Ñ‹Ğ¼Ğ¸ Ğ°Ñ€Ğ³ÑƒĞ¼ĞµĞ½Ñ‚Ğ°Ğ¼Ğ¸
                    remote:InvokeServer({
                        Action = "KICK_PLAYER",
                        Target = targetPlayer.Name,
                        Evidence = {CheatDetected = true}
                    })
                end
            end)
        end
        
        return #criticalRemotes > 0
    end
}

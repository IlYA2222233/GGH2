-- –§—É–Ω–∫—Ü–∏–∏ –ª–µ–≥–∞–ª—å–Ω–æ–≥–æ –∞–Ω—Ç–∏-—á–∏—Ç–∞

-- 1. –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –Ω–µ—Ä–µ–∞–ª—å–Ω—É—é —Å–∫–æ—Ä–æ—Å—Ç—å
local function checkUnrealisticSpeed(player)
    if not player.Character then return false end
    
    local humanoid = player.Character:FindFirstChild("Humanoid")
    local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
    
    if humanoid and humanoidRootPart then
        -- –õ–æ–≥–∏—Ä—É–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å (—Ç–æ–ª—å–∫–æ –¥–ª—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏)
        local velocity = humanoidRootPart.Velocity
        local speed = velocity.Magnitude
        
        -- –§–∏–∑–∏—á–µ—Å–∫–∏–µ –ø—Ä–µ–¥–µ–ª—ã —Å–∫–æ—Ä–æ—Å—Ç–∏ –≤ Roblox
        if speed > 500 then -- –û—á–µ–Ω—å –≤—ã—Å–æ–∫–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å
            return {isCheating = true, reason = "–ù–µ—Ä–µ–∞–ª—å–Ω–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å: " .. math.floor(speed) .. " studs/s", confidence = 0.8}
        end
    end
    
    return {isCheating = false, reason = "–°–∫–æ—Ä–æ—Å—Ç—å –≤ –Ω–æ—Ä–º–µ", confidence = 0.1}
end

-- 2. –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ –ª–µ–≤–∏—Ç–∞—Ü–∏—é (–Ω–µ–∑–∞–∫–æ–Ω–Ω—ã–π –ø–æ–ª–µ—Ç)
local function checkIllegalFlying(player)
    if not player.Character then return false end
    
    local humanoid = player.Character:FindFirstChild("Humanoid")
    local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
    
    if humanoid and humanoidRootPart then
        -- –ü—Ä–æ–≤–µ—Ä–∫–∞: –∏–≥—Ä–æ–∫ –≤ –≤–æ–∑–¥—É—Ö–µ –±–µ–∑ –≥—Ä–∞–≤–∏—Ç–∞—Ü–∏–∏
        local seat = humanoid.SeatPart
        local hasSeat = seat ~= nil
        
        -- –ï—Å–ª–∏ –∏–≥—Ä–æ–∫ –≤ –≤–æ–∑–¥—É—Ö–µ –±–µ–∑ —Å–∏–¥–µ–Ω–∏—è –∏ –∏–º–µ–µ—Ç –Ω–µ–æ–±—ã—á–Ω—É—é –≤—ã—Å–æ—Ç—É
        if not hasSeat and humanoidRootPart.Position.Y > 1000 then
            return {isCheating = true, reason = "–ù–µ–∑–∞–∫–æ–Ω–Ω—ã–π –ø–æ–ª–µ—Ç –Ω–∞ –≤—ã—Å–æ—Ç–µ " .. math.floor(humanoidRootPart.Position.Y), confidence = 0.9}
        end
    end
    
    return {isCheating = false, reason = "–ü–æ–ª–µ—Ç –ª–µ–≥–∞–ª—å–Ω—ã–π", confidence = 0.1}
end

-- 3. –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—é
local function checkTeleportation(player, lastPosition)
    if not player.Character then return lastPosition, false end
    
    local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return lastPosition, false end
    
    local currentPosition = humanoidRootPart.Position
    
    if lastPosition then
        local distance = (currentPosition - lastPosition).Magnitude
        
        -- –ï—Å–ª–∏ –∏–≥—Ä–æ–∫ –ø–µ—Ä–µ–º–µ—Å—Ç–∏–ª—Å—è –Ω–∞ –æ–≥—Ä–æ–º–Ω–æ–µ —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –∑–∞ 1 –∫–∞–¥—Ä
        if distance > 500 then -- –§–∏–∑–∏—á–µ—Å–∫–∏ –Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ–µ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ
            return currentPosition, {isCheating = true, reason = "–¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è: " .. math.floor(distance) .. " studs –∑–∞ –∫–∞–¥—Ä", confidence = 0.95}
        end
    end
    
    return currentPosition, {isCheating = false, reason = "–ü–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ –ª–µ–≥–∞–ª—å–Ω–æ–µ", confidence = 0.1}
end

-- 4. –ü—Ä–æ–≤–µ—Ä–∫–∞ –Ω–∞ Noclip
local function checkNoclip(player)
    if not player.Character then return false end
    
    local parts = player.Character:GetChildren()
    local clippingParts = 0
    local totalParts = 0
    
    for _, part in ipairs(parts) do
        if part:IsA("BasePart") and part.CanCollide == false then
            clippingParts = clippingParts + 1
        end
        if part:IsA("BasePart") then
            totalParts = totalParts + 1
        end
    end
    
    -- –ï—Å–ª–∏ —É –∏–≥—Ä–æ–∫–∞ –≤—Å–µ —á–∞—Å—Ç–∏ –Ω–µ –∫–æ–ª–ª–∞–π–¥–µ—Ä–Ω—ã–µ
    if totalParts > 0 and clippingParts == totalParts then
        return {isCheating = true, reason = "Noclip –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω", confidence = 0.85}
    end
    
    return {isCheating = false, reason = "–ö–æ–ª–ª–∏–∑–∏–∏ –≤ –Ω–æ—Ä–º–µ", confidence = 0.1}
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–∞ —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
local function createPlayerListItem(player, index)
    local itemFrame = Instance.new("Frame")
    itemFrame.Size = UDim2.new(1, -10, 0, 60)
    itemFrame.Position = UDim2.new(0, 5, 0, (index-1)*65)
    itemFrame.BackgroundColor3 = Color3.fromRGB(45, 50, 65)
    itemFrame.BorderSizePixel = 0
    
    local itemCorner = Instance.new("UICorner")
    itemCorner.CornerRadius = UDim.new(0, 8)
    itemCorner.Parent = itemFrame
    
    local playerName = Instance.new("TextLabel")
    playerName.Text = player.Name
    playerName.Size = UDim2.new(0.7, 0, 0.5, 0)
    playerName.Position = UDim2.new(0.05, 0, 0, 0)
    playerName.BackgroundTransparency = 1
    playerName.TextColor3 = Color3.new(1, 1, 1)
    playerName.Font = Enum.Font.Gotham
    playerName.TextSize = 16
    playerName.TextXAlignment = Enum.TextXAlignment.Left
    playerName.Parent = itemFrame
    
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Text = "‚úÖ –ü—Ä–æ–≤–µ—Ä—è–µ—Ç—Å—è..."
    statusLabel.Size = UDim2.new(0.9, 0, 0.5, 0)
    statusLabel.Position = UDim2.new(0.05, 0, 0.5, 0)
    statusLabel.BackgroundTransparency = 1
    statusLabel.TextColor3 = Color3.fromRGB(180, 180, 220)
    statusLabel.Font = Enum.Font.Gotham
    statusLabel.TextSize = 12
    statusLabel.TextXAlignment = Enum.TextXAlignment.Left
    statusLabel.Parent = itemFrame
    
    -- –ö–Ω–æ–ø–∫–∞ "–ü–æ–∂–∞–ª–æ–≤–∞—Ç—å—Å—è" (–æ—Ñ–∏—Ü–∏–∞–ª—å–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è)
    local reportButton = Instance.new("TextButton")
    reportButton.Text = "üìã –ü–æ–∂–∞–ª–æ–≤–∞—Ç—å—Å—è"
    reportButton.Size = UDim2.new(0.25, 0, 0.7, 0)
    reportButton.Position = UDim2.new(0.72, 0, 0.15, 0)
    reportButton.BackgroundColor3 = Color3.fromRGB(60, 70, 90)
    reportButton.TextColor3 = Color3.new(1, 1, 1)
    reportButton.Font = Enum.Font.Gotham
    reportButton.TextSize = 12
    reportButton.Parent = itemFrame
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 6)
    buttonCorner.Parent = reportButton
    
    reportButton.MouseButton1Click:Connect(function()
        -- –õ–µ–≥–∞–ª—å–Ω—ã–π —Å–ø–æ—Å–æ–±: –ø—Ä–µ–¥–ª–æ–∂–∏—Ç—å –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å—Å—è —á–µ—Ä–µ–∑ –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω—É—é —Å–∏—Å—Ç–µ–º—É
        local message = string.format("–ò–≥—Ä–æ–∫ %s –≤–æ–∑–º–æ–∂–Ω—ã–π —á–∏—Ç–µ—Ä. –°–∫–æ—Ä–æ—Å—Ç—å –ø—Ä–æ–≤–µ—Ä–µ–Ω–∞.", player.Name)
        warn("–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏—è: –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω—É—é —Å–∏—Å—Ç–µ–º—É –∂–∞–ª–æ–± Roblox –¥–ª—è –∏–≥—Ä–æ–∫–∞:", player.Name)
    end)
    
    return itemFrame, statusLabel
end

-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –º–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥–∞
local function startMonitoring()
    local playerLastPositions = {}
    local suspectCount = 0
    
    -- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
    local playerItems = {}
    local playerStatusLabels = {}
    
    local function updatePlayerList()
        local players = Players:GetPlayers()
        local yOffset = 0
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É
        TotalPlayersLabel.Text = "–ò–≥—Ä–æ–∫–æ–≤ –æ–Ω–ª–∞–π–Ω: " .. #players
        
        -- –û—á–∏—â–∞–µ–º —Å–ø–∏—Å–æ–∫
        for _, item in pairs(playerItems) do
            item:Destroy()
        end
        playerItems = {}
        playerStatusLabels = {}
        
        -- –°–æ–∑–¥–∞–µ–º –Ω–æ–≤—ã–µ —ç–ª–µ–º–µ–Ω—Ç—ã
        for i, player in ipairs(players) do
            if player ~= LocalPlayer then
                local item, statusLabel = createPlayerListItem(player, i)
                item.Parent = PlayersScrollingFrame
                playerItems[player] = item
                playerStatusLabels[player] = statusLabel
                yOffset = yOffset + 65
            end
        end
        
        PlayersScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, yOffset)
    end
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º —Å–ø–∏—Å–æ–∫ –ø—Ä–∏ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–∏/–æ—Ç–∫–ª—é—á–µ–Ω–∏–∏ –∏–≥—Ä–æ–∫–æ–≤
    Players.PlayerAdded:Connect(updatePlayerList)
    Players.PlayerRemoving:Connect(updatePlayerList)
    updatePlayerList()
    
    -- –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª –º–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥–∞
    RunService.Heartbeat:Connect(function()
        suspectCount = 0
        
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                -- –ü—Ä–æ–≤–µ—Ä—è–µ–º –≤—Å–µ –≤–∏–¥—ã —á–∏—Ç–∞
                local speedCheck = checkUnrealisticSpeed(player)
                local flyCheck = checkIllegalFlying(player)
                local lastPos = playerLastPositions[player]
                local currentPos, teleportCheck = checkTeleportation(player, lastPos)
                local noclipCheck = checkNoclip(player)
                
                playerLastPositions[player] = currentPos
                
                -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç—É—Å –≤ GUI
                local statusLabel = playerStatusLabels[player]
                if statusLabel then
                    if speedCheck.isCheating then
                        statusLabel.Text = "‚ö†Ô∏è –ù–µ—Ä–µ–∞–ª—å–Ω–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å"
                        statusLabel.TextColor3 = Color3.fromRGB(255, 150, 50)
                        suspectCount = suspectCount + 1
                    elseif flyCheck.isCheating then
                        statusLabel.Text = "‚ö†Ô∏è –ù–µ–∑–∞–∫–æ–Ω–Ω—ã–π –ø–æ–ª–µ—Ç"
                        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
                        suspectCount = suspectCount + 1
                    elseif teleportCheck.isCheating then
                        statusLabel.Text = "‚ö†Ô∏è –¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è"
                        statusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
                        suspectCount = suspectCount + 1
                    elseif noclipCheck.isCheating then
                        statusLabel.Text = "‚ö†Ô∏è Noclip"
                        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 150)
                        suspectCount = suspectCount + 1
                    else
                        statusLabel.Text = "‚úÖ –õ–µ–≥–∞–ª—å–Ω—ã–π –∏–≥—Ä–æ–∫"
                        statusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
                    end
                end
            end
        end
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—á–µ—Ç—á–∏–∫ –ø–æ–¥–æ–∑—Ä–∏—Ç–µ–ª—å–Ω—ã—Ö
        SuspectsLabel.Text = "–ü–æ–¥–æ–∑—Ä–∏—Ç–µ–ª—å–Ω—ã—Ö: " .. suspectCount
    end)
    
    -- –ö–Ω–æ–ø–∫–∞ "–û–±–Ω–æ–≤–∏—Ç—å —Å–ø–∏—Å–æ–∫"
    local RefreshButton = Instance.new("TextButton")
    RefreshButton.Text = "üîÑ –û–±–Ω–æ–≤–∏—Ç—å —Å–ø–∏—Å–æ–∫"
    RefreshButton.Size = UDim2.new(0.9, 0, 0, 40)
    RefreshButton.Position = UDim2.new(0.05, 0, 0.85, 0)
    RefreshButton.BackgroundColor3 = Color3.fromRGB(50, 100, 180)
    RefreshButton.TextColor3 = Color3.new(1, 1, 1)
    RefreshButton.Font = Enum.Font.GothamBold
    RefreshButton.TextSize = 16
    RefreshButton.Parent = MainFrame
    
    local refreshCorner = Instance.new("UICorner")
    refreshCorner.CornerRadius = UDim.new(0, 10)
    refreshCorner.Parent = RefreshButton
    
    RefreshButton.MouseButton1Click:Connect(updatePlayerList)
    
    -- –ò–Ω—Ñ–æ—Ä–º–∞—Ü–∏–æ–Ω–Ω–∞—è –ø–∞–Ω–µ–ª—å
    local InfoLabel = Instance.new("TextLabel")
    InfoLabel.Text = "‚ÑπÔ∏è –°–∏—Å—Ç–µ–º–∞ —Ç–æ–ª—å–∫–æ –æ–±–Ω–∞—Ä—É–∂–∏–≤–∞–µ—Ç. –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ –æ—Ñ–∏—Ü–∏–∞–ª—å–Ω—ã–µ –∂–∞–ª–æ–±—ã Roblox."
    InfoLabel.Size = UDim2.new(0.9, 0, 0, 40)
    InfoLabel.Position = UDim2.new(0.05, 0, 0.92, 0)
    InfoLabel.BackgroundTransparency = 1
    InfoLabel.TextColor3 = Color3.fromRGB(150, 150, 200)
    InfoLabel.Font = Enum.Font.Gotham
    InfoLabel.TextSize = 12
    InfoLabel.TextWrapped = true
    InfoLabel.Parent = MainFrame
end

-- –ó–∞–ø—É—Å–∫–∞–µ–º —Å–∏—Å—Ç–µ–º—É
startMonitoring()

-- –ê–Ω–∏–º–∞—Ü–∏—è –ø–æ—è–≤–ª–µ–Ω–∏—è
MainFrame.Visible = true
task.wait(0.1)
MainFrame.Visible = false


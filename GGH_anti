-- –§—É–Ω–∫—Ü–∏–∏ –ª–µ–≥–∞–ª—å–Ω–æ–≥–æ –∞–Ω—Ç–∏-—á–∏—Ç–∞
local function checkUnrealisticSpeed(player)
    if not player.Character then return false end
    
    local humanoid = player.Character:FindFirstChild("Humanoid")
    local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
    
    if humanoid and humanoidRootPart then
        local velocity = humanoidRootPart.Velocity
        local speed = velocity.Magnitude
        
        if speed > 500 then
            return {isCheating = true, reason = "–ù–µ—Ä–µ–∞–ª—å–Ω–∞—è —Å–∫–æ—Ä–æ—Å—Ç—å", confidence = 0.8}
        end
    end
    
    return {isCheating = false, reason = "–ù–æ—Ä–º–∞", confidence = 0.1}
end

local function checkIllegalFlying(player)
    if not player.Character then return false end
    
    local humanoid = player.Character:FindFirstChild("Humanoid")
    local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
    
    if humanoid and humanoidRootPart then
        local seat = humanoid.SeatPart
        local hasSeat = seat ~= nil
        
        if not hasSeat and humanoidRootPart.Position.Y > 1000 then
            return {isCheating = true, reason = "–ù–µ–∑–∞–∫–æ–Ω–Ω—ã–π –ø–æ–ª–µ—Ç", confidence = 0.9}
        end
    end
    
    return {isCheating = false, reason = "–ù–æ—Ä–º–∞", confidence = 0.1}
end

local playerLastPositions = {}
local function checkTeleportation(player)
    if not player.Character then return false end
    
    local humanoidRootPart = player.Character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then return false end
    
    local currentPosition = humanoidRootPart.Position
    local lastPos = playerLastPositions[player]
    
    if lastPos then
        local distance = (currentPosition - lastPos).Magnitude
        
        if distance > 500 then
            return {isCheating = true, reason = "–¢–µ–ª–µ–ø–æ—Ä—Ç–∞—Ü–∏—è", confidence = 0.95}
        end
    end
    
    playerLastPositions[player] = currentPosition
    return {isCheating = false, reason = "–ù–æ—Ä–º–∞", confidence = 0.1}
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è —Å–æ–∑–¥–∞–Ω–∏—è —ç–ª–µ–º–µ–Ω—Ç–∞ —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
local function createPlayerListItem(player, index)
    local itemFrame = Instance.new("Frame")
    itemFrame.Size = UDim2.new(1, -10, 0, 70)
    itemFrame.Position = UDim2.new(0, 5, 0, (index-1)*75)
    itemFrame.BackgroundColor3 = Color3.fromRGB(40, 45, 65)
    itemFrame.BackgroundTransparency = 0.1
    itemFrame.ZIndex = 103
    itemFrame.BorderSizePixel = 0
    
    local itemCorner = Instance.new("UICorner")
    itemCorner.CornerRadius = UDim.new(0, 10)
    itemCorner.Parent = itemFrame
    
    -- –ê–≤–∞—Ç–∞—Ä –∏–≥—Ä–æ–∫–∞
    local avatar = Instance.new("ImageLabel")
    avatar.Size = UDim2.new(0, 50, 0, 50)
    avatar.Position = UDim2.new(0.02, 0, 0.15, 0)
    avatar.BackgroundColor3 = Color3.fromRGB(60, 70, 90)
    avatar.ZIndex = 104
    avatar.Parent = itemFrame
    
    local avatarCorner = Instance.new("UICorner")
    avatarCorner.CornerRadius = UDim.new(0, 25)
    avatarCorner.Parent = avatar
    
    -- –ò–º—è –∏–≥—Ä–æ–∫–∞
    local playerName = Instance.new("TextLabel")
    playerName.Text = player.Name
    playerName.Size = UDim2.new(0.6, 0, 0.4, 0)
    playerName.Position = UDim2.new(0.18, 0, 0, 0)
    playerName.BackgroundTransparency = 1
    playerName.TextColor3 = Color3.new(1, 1, 1)
    playerName.Font = Enum.Font.GothamBold
    playerName.TextSize = 16
    playerName.TextXAlignment = Enum.TextXAlignment.Left
    playerName.ZIndex = 104
    playerName.Parent = itemFrame
    
    local statusLabel = Instance.new("TextLabel")
    statusLabel.Text = "üîç –ê–Ω–∞–ª–∏–∑..."
    statusLabel.Size = UDim2.new(0.6, 0, 0.4, 0)
    statusLabel.Position = UDim2.new(0.18, 0, 0.4, 0)
    statusLabel.BackgroundTransparency = 1
    statusLabel.TextColor3 = Color3.fromRGB(180, 200, 255)
    statusLabel.Font = Enum.Font.Gotham
    statusLabel.TextSize = 13
    statusLabel.TextXAlignment = Enum.TextXAlignment.Left
    statusLabel.ZIndex = 104
    statusLabel.Parent = itemFrame
    
    -- –ö–Ω–æ–ø–∫–∞ –¥–µ–π—Å—Ç–≤–∏—è
    local actionButton = Instance.new("TextButton")
    actionButton.Text = "üìã –û—Ç—á—ë—Ç"
    actionButton.Size = UDim2.new(0.25, 0, 0.7, 0)
    actionButton.Position = UDim2.new(0.72, 0, 0.15, 0)
    actionButton.BackgroundColor3 = Color3.fromRGB(60, 80, 160)
    actionButton.TextColor3 = Color3.new(1, 1, 1)
    actionButton.Font = Enum.Font.GothamBold
    actionButton.TextSize = 12
    actionButton.ZIndex = 104
    actionButton.Parent = itemFrame
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 8)
    buttonCorner.Parent = actionButton
    
    -- –≠—Ñ—Ñ–µ–∫—Ç –ø—Ä–∏ –Ω–∞–≤–µ–¥–µ–Ω–∏–∏
    actionButton.MouseEnter:Connect(function()
        actionButton.BackgroundColor3 = Color3.fromRGB(80, 100, 200)
    end)
    
    actionButton.MouseLeave:Connect(function()
        actionButton.BackgroundColor3 = Color3.fromRGB(60, 80, 160)
    end)
    
    actionButton.MouseButton1Click:Connect(function()
        -- –õ–µ–≥–∞–ª—å–Ω—ã–π —Å–ø–æ—Å–æ–±: —Å–æ–∑–¥–∞–Ω–∏–µ –æ—Ç—á–µ—Ç–∞
        StarterGui:SetCore("SendNotification", {
            Title = "GGH Report",
            Text = "–û—Ç—á–µ—Ç —Å–æ–∑–¥–∞–Ω –¥–ª—è: " .. player.Name,
            Duration = 3,
            Icon = "rbxassetid://4483345998"
        })
    end)
    
    return itemFrame, statusLabel, avatar
end

-- –û—Å–Ω–æ–≤–Ω–∞—è —Ñ—É–Ω–∫—Ü–∏—è –º–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥–∞
local function startMonitoring()
    local suspectCount = 0
    local playerItems = {}
    local playerStatusLabels = {}
    local playerAvatars = {}
    
    local function updatePlayerList()
        local players = Players:GetPlayers()
        local yOffset = 0
        
        TotalPlayersLabel.Text = "üéÆ –ò–≥—Ä–æ–∫–æ–≤ –æ–Ω–ª–∞–π–Ω: " .. #players
        
        for _, item in pairs(playerItems) do
            item:Destroy()
        end
        
        playerItems = {}
        playerStatusLabels = {}
        playerAvatars = {}
        
        for i, player in ipairs(players) do
            if player ~= LocalPlayer then
                local item, statusLabel, avatar = createPlayerListItem(player, i)
                item.Parent = PlayersScrollingFrame
                playerItems[player] = item
                playerStatusLabels[player] = statusLabel
                playerAvatars[player] = avatar
                yOffset = yOffset + 75
            end
        end
        
        PlayersScrollingFrame.CanvasSize = UDim2.new(0, 0, 0, yOffset)
    end
    
    Players.PlayerAdded:Connect(updatePlayerList)
    Players.PlayerRemoving:Connect(updatePlayerList)
    updatePlayerList()
    
    -- –ú–æ–Ω–∏—Ç–æ—Ä–∏–Ω–≥ –≤ —Ä–µ–∞–ª—å–Ω–æ–º –≤—Ä–µ–º–µ–Ω–∏
    RunService.Heartbeat:Connect(function()
        suspectCount = 0
        
        for _, player in ipairs(Players:GetPlayers()) do
            if player ~= LocalPlayer then
                local speedCheck = checkUnrealisticSpeed(player)
                local flyCheck = checkIllegalFlying(player)
                local teleportCheck = checkTeleportation(player)
                
                local statusLabel = playerStatusLabels[player]
                
                if statusLabel then
                    if speedCheck.isCheating then
                        statusLabel.Text = "‚ö° –°–ö–û–†–û–°–¢–ù–û–ô –ß–ò–¢"
                        statusLabel.TextColor3 = Color3.fromRGB(255, 150, 50)
                        suspectCount = suspectCount + 1
                    elseif flyCheck.isCheating then
                        statusLabel.Text = "üïäÔ∏è –õ–ï–¢–ê–Æ–©–ò–ô –ß–ò–¢"
                        statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
                        suspectCount = suspectCount + 1
                    elseif teleportCheck.isCheating then
                        statusLabel.Text = "üåÄ –¢–ï–õ–ï–ü–û–†–¢ –ß–ò–¢"
                        statusLabel.TextColor3 = Color3.fromRGB(255, 50, 50)
                        suspectCount = suspectCount + 1
                    else
                        statusLabel.Text = "‚úÖ –õ–ï–ì–ò–¢ –ò–ì–†–û–ö"
                        statusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
                    end
                end
            end
        end
        
        SuspectsLabel.Text = "‚ö†Ô∏è –ü–æ–¥–æ–∑—Ä–∏—Ç–µ–ª—å–Ω—ã—Ö: " .. suspectCount
        
        -- –ú–∏–≥–∞–Ω–∏–µ –ø—Ä–∏ –æ–±–Ω–∞—Ä—É–∂–µ–Ω–∏–∏ —á–∏—Ç–∞
        if suspectCount > 0 then
            Title.Text = "üî¥ GGH ANTI-CHEAT [" .. suspectCount .. "]"
            Title.TextColor3 = Color3.fromRGB(255, 100, 100)
        else
            Title.Text = "üîí GGH ANTI-CHEAT SYSTEM"
            Title.TextColor3 = Color3.new(1, 1, 1)
        end
    end)
    
    -- –ö–Ω–æ–ø–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
    local RefreshButton = Instance.new("TextButton")
    RefreshButton.Text = "üîÑ –û–ë–ù–û–í–ò–¢–¨ –î–ê–ù–ù–´–ï"
    RefreshButton.Size = UDim2.new(0.9, 0, 0, 50)
    RefreshButton.Position = UDim2.new(0.05, 0, 0.9, 0)
    RefreshButton.BackgroundColor3 = Color3.fromRGB(60, 90, 180)
    RefreshButton.TextColor3 = Color3.new(1, 1, 1)
    RefreshButton.Font = Enum.Font.GothamBlack
    RefreshButton.TextSize = 16
    RefreshButton.ZIndex = 101
    RefreshButton.Parent = MainFrame
    
    local refreshCorner = Instance.new("UICorner")
    refreshCorner.CornerRadius = UDim.new(0, 12)
    refreshCorner.Parent = RefreshButton
    
    RefreshButton.MouseButton1Click:Connect(updatePlayerList)
    
    -- –ò–Ω—Ñ–æ –ø–∞–Ω–µ–ª—å
    local InfoLabel = Instance.new("TextLabel")
    InfoLabel.Text = "üìå GGH Anti-Cheat v2.0 | GUI Surface Layer | –õ–µ–≥–∞–ª—å–Ω–∞—è —Å–∏—Å—Ç–µ–º–∞ –∞–Ω–∞–ª–∏–∑–∞"
    InfoLabel.Size = UDim2.new(0.9, 0, 0, 25)
    InfoLabel.Position = UDim2.new(0.05, 0, 0.97, 0)
    InfoLabel.BackgroundTransparency = 1
    InfoLabel.TextColor3 = Color3.fromRGB(150, 170, 220)
    InfoLabel.Font = Enum.Font.Gotham
    InfoLabel.TextSize = 11
    InfoLabel.TextWrapped = true
    InfoLabel.ZIndex = 101
    InfoLabel.Parent = MainFrame
    
    -- –ê–Ω–∏–º–∞—Ü–∏—è –∑–∞–≥—Ä—É–∑–∫–∏
    local loadingText = Instance.new("TextLabel")
    loadingText.Text = "GGH Anti-Cheat –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω..."
    loadingText.Size = UDim2.new(1, 0, 0, 30)
    loadingText.Position = UDim2.new(0, 0, 1.05, 0)
    loadingText.BackgroundTransparency = 1
    loadingText.TextColor3 = Color3.fromRGB(100, 255, 100)
    loadingText.Font = Enum.Font.GothamBold
    loadingText.TextSize = 14
    loadingText.ZIndex = 101
    loadingText.Parent = MainFrame
    
    task.spawn(function()
        for i = 1, 10 do
            loadingText.Text = "GGH Anti-Cheat –∞–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω" .. string.rep(".", i % 4)
            task.wait(0.3)
        end
        loadingText.Visible = false
    end)
end

-- –ó–∞–ø—É—Å–∫ —Å–∏—Å—Ç–µ–º—ã
startMonitoring()

-- –ì–æ—Ä—è—á–∏–µ –∫–ª–∞–≤–∏—à–∏ –ø–æ–º–æ—â–∏
UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.H then
        StarterGui:SetCore("SendNotification", {
            Title = "GGH –ü–æ–º–æ—â—å",
            Text = "K - –ú–µ–Ω—é | ESC - –ó–∞–∫—Ä—ã—Ç—å | H - –ü–æ–º–æ—â—å",
            Duration = 5,
            Icon = "rbxassetid://4483345998"
        })
    end
end)

print("üöÄ GGH Anti-Cheat System –∑–∞–≥—Ä—É–∂–µ–Ω!")
print("üéØ –ù–∞–∂–º–∏—Ç–µ K –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è –º–µ–Ω—é")
print("üîù GUI –Ω–∞ —Å–∞–º–æ–º –≤–µ—Ä—Ö–Ω–µ–º —Å–ª–æ–µ")
